<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <configSections>

  </configSections>
  <appSettings>
  </appSettings>
  <system.web>
  </system.web>
   <system.webServer>
    <rewrite>
      <rules>
        <rule name="Rewrite" stopProcessing="true">
         <match url=".*" />
			<conditions>
				<add input="{HTTP_HOST}" pattern="^(.*).(tbgfeb\.dolphin\.berndtgroup\.net)$" />
			</conditions>
			<action type="Rewrite" url="/{C:1}/{R:0}" />
        </rule>
	</rules>
	</rewrite>
        <staticContent>
            <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
            <mimeMap fileExtension=".mp4" mimeType="video/mp4" />
        </staticContent>
	</system.webServer>
</configuration>